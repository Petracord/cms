{
	"name": "@petracord/cms",
	"version": "0.0.1",
	"main": "dist/server.js",
	"license": "MIT",
	"scripts": {
		"update": "yarn upgrade-interactive",
		"dd": "yarn dedupe --strategy highest",
		"dev": "tsx watch src/server.ts",
		"build:payload": "payload build",
		"build:server": "tsup",
		"build": "yarn copyfiles && yarn build:payload && yarn build:server",
		"serve": "NODE_ENV=production node dist/server.js",
		"copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
		"generate:types": "payload generate:types",
		"generate:graphQLSchema": "payload generate:graphQLSchema",
		"push": "payload migrate",
		"migrate": "payload migrate:create",
		"lint": "eslint --ext ts src",
		"lint:fix": "eslint --ext ts --fix src",
		"format": "prettier --check .",
		"format:fix": "prettier --write --log-level=warn ."
	},
	"dependencies": {
		"@payloadcms/bundler-webpack": "^1.0.5",
		"@payloadcms/db-postgres": "^0.2.1",
		"@payloadcms/plugin-cloud": "^2.2.6",
		"@payloadcms/richtext-slate": "^1.3.1",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"payload": "^2.4.0"
	},
	"devDependencies": {
		"@kbotdev/eslint-config": "^2.0.1",
		"@kbotdev/prettier-config": "^1.0.2",
		"@kbotdev/ts-config": "^1.0.2",
		"@types/eslint": "^8.44.9",
		"@types/express": "^4.17.21",
		"@types/node": "^20.10.4",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"copyfiles": "^2.4.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.0.1",
		"prettier": "^3.1.1",
		"tsup": "^8.0.1",
		"tsx": "^4.6.2",
		"typescript": "^5.3.3"
	},
	"packageManager": "yarn@4.0.2"
}
